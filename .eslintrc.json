{
  "env": {
      "node": true,
      "browser": true,
      "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": "off",
    "semi": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "no-use-before-define": "off"
  },
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "prettier/flowtype",
    "prettier/react",
    "prettier/standard"
  ]
}